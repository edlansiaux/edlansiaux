    "# Configuration des graphes\n",
    "plt.style.use('seaborn-v0_8-whitegrid')\n",
    "sns.set_palette(\"husl\")\n",
    "\n",
    "# Paramètres de simulation\n",
    "np.random.seed(2026)"
