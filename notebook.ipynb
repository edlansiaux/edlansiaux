    "## 6. Calcul des indicateurs pour tous les fichiers et scénarios"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "def analyser_risques(fichiers, scenarios, variables_inference, variables_correlation):\n",
    "    \"\"\"Calcule les indicateurs de risque pour tous les fichiers et scénarios\"\"\"\n",
    "    \n",
    "    resultats = []\n",
    "    base_reference = fichiers['connaissances_externes']\n",
    "    \n",
    "    for nom_fichier, df_anon in fichiers.items():\n",
    "        # Extraire le type et le niveau de mélange\n",
    "        if nom_fichier == 'connaissances_externes':\n",
    "            type_fichier = 'reference'\n",
    "            niveau_melange = 0\n",
    "        elif 'direct' in nom_fichier:\n",
    "            type_fichier = 'direct'\n",
    "            niveau_melange = int(nom_fichier.split('_')[-1])\n",
    "        else:\n",
    "            type_fichier = 'sample'\n",
    "            niveau_melange = int(nom_fichier.split('_')[-1])\n",
    "        \n",
    "        for scenario in scenarios:\n",
    "            variables = scenario['variables']\n",
    "            \n",
    "            # Calcul des indicateurs\n",
    "            unicite = IndicateursRisque.calculate_unicite(df_anon, variables)\n",
    "            k_anonymat = IndicateursRisque.calculate_k_anonymat(df_anon, variables, k=2)\n",
    "            entropie = IndicateursRisque.calculate_entropie(df_anon, variables)\n",
    "            risque_inference = IndicateursRisque.calculate_risque_inference(\n",
    "                base_reference, df_anon, variables_inference)\n",
    "            risque_correlation = IndicateursRisque.calculate_risque_correlation(\n",
    "                base_reference, df_anon, variables_correlation)\n",
    "            risque_individualisation = IndicateursRisque.calculate_risque_individualisation(\n",
    "                base_reference, df_anon, variables)\n",
    "            \n",
    "            resultats.append({\n",
    "                'fichier': nom_fichier,\n",
    "                'type': type_fichier,\n",
    "                'niveau_melange': niveau_melange,\n",
    "                'scenario': scenario['nom'],\n",
    "                'n_variables': len(variables),\n",
    "                'unicite': unicite,\n",
    "                'k_anonymat': k_anonymat,\n",
    "                'entropie': entropie,\n",
    "                'risque_inference': risque_inference,\n",
    "                'risque_correlation': risque_correlation,\n",
    "                'risque_individualisation': risque_individualisation\n",
    "            })\n",
    "    \n",
    "    return pd.DataFrame(resultats)"
   ]
  },
