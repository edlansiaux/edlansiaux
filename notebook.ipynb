    "def melanger_colonnes(df, pourcentage_melange=20):\n",
    "    \"\"\"Mélange un pourcentage des cellules dans chaque colonne\"\"\"\n",
    "    df_melange = df.copy()\n",
    "    \n",
    "    for col in df.columns:\n",
    "        if col == 'id_sejour':\n",
    "            continue  # Ne pas mélanger l'identifiant\n",
    "            \n",
    "        n_cells = len(df)\n",
    "        n_melanger = int(n_cells * pourcentage_melange / 100)\n",
    "        \n",
    "        if n_melanger > 0:\n",
    "            indices = np.random.choice(n_cells, n_melanger, replace=False)\n",
    "            valeurs = df[col].iloc[indices].copy()\n",
    "            np.random.shuffle(valeurs.values)\n",
    "            df_melange.loc[indices, col] = valeurs.values\n",
    "    \n",
    "    return df_melange\n",
    "\n",
    "def echantillonner_avec_remise(df, taille_echantillon=None, pourcentage_melange=20):\n",
    "    \"\"\"Échantillonne avec remise puis mélange\"\"\"\n",
    "    if taille_echantillon is None:\n",
    "        taille_echantillon = len(df)\n",
    "    \n",
    "    indices = np.random.choice(len(df), taille_echantillon, replace=True)\n",
    "    df_echantillon = df.iloc[indices].reset_index(drop=True)\n",
    "    \n",
    "    # Recréer les id_sejour pour correspondre à l'original\n",
    "    df_echantillon['id_sejour'] = df['id_sejour'].iloc[indices].values\n",
    "    \n",
    "    # Appliquer le mélange\n",
    "    df_echantillon = melanger_colonnes(df_echantillon, pourcentage_melange)\n",
    "    \n",
    "    return df_echantillon\n",
    "\n",
    "def generer_fichiers(base_reference, niveaux_melange=[0, 10, 20, 30, 40, 50]):\n",
    "    \"\"\"Génère les différents fichiers pour l'analyse\"\"\"\n",
    "    fichiers = {}\n",
    "    \n",
    "    # connaissances_externes.txt (base de référence)\n",
    "    fichiers['connaissances_externes'] = base_reference\n",
    "    \n",
    "    # out_direct_XX.txt\n",
    "    for niveau in niveaux_melange:\n",
    "        fichiers[f'out_direct_{niveau}'] = melanger_colonnes(base_reference, niveau)\n",
    "    \n",
    "    # out_sample_XX.txt\n",
    "    for niveau in niveaux_melange:\n",
    "        fichiers[f'out_sample_{niveau}'] = echantillonner_avec_remise(base_reference, \n",
    "                                                                     len(base_reference), \n",
    "                                                                     niveau)\n",
    "    \n",
    "    return fichiers"
   ]
