    "def definir_scenarios():\n",
    "    \"\"\"Définit différents scénarios de connaissances externes\"\"\"\n",
    "    scenarios = [\n",
    "        {\n",
    "            'nom': 'Scénario 1 (basique)',\n",
    "            'variables': ['age10', 'sexe', 'entree_date_y']\n",
    "        },\n",
    "        {\n",
    "            'nom': 'Scénario 2 (intermédiaire)',\n",
    "            'variables': ['age5', 'sexe', 'entree_date_ym', 'specialite']\n",
    "        },\n",
    "        {\n",
    "            'nom': 'Scénario 3 (avancé)',\n",
    "            'variables': ['age', 'sexe', 'entree_date_ymd', 'specialite', 'chirurgie']\n",
    "        },\n",
    "        {\n",
    "            'nom': 'Scénario 4 (expert)',\n",
    "            'variables': ['age', 'sexe', 'entree_date_ymd', 'specialite', \n",
    "                         'chirurgie', 'diabete', 'liste_diag']\n",
    "        }\n",
    "    ]\n",
    "    \n",
    "    # Variables pour les différents types de risque\n",
    "    variables_inference = ['diabete', 'demence', 'insuffisance_renale']\n",
    "    variables_correlation = ['age', 'sexe', 'specialite', 'chirurgie']\n",
    "    \n",
    "    return scenarios, variables_inference, variables_correlation"
   ]
