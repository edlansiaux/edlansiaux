   "source": [
    "def simuler_base_donnees(n=1000):\n",
    "    \"\"\"Simule une base de données hospitalière fictive\"\"\"\n",
    "    \n",
    "    df = pd.DataFrame()\n",
    "    \n",
    "    # Identifiant\n",
    "    df['id_sejour'] = range(1, n+1)\n",
    "    \n",
    "    # Âge (avec différentes précisions)\n",
    "    ages = np.random.randint(18, 95, n)\n",
    "    df['age'] = ages\n",
    "    df['age5'] = (ages // 5) * 5\n",
    "    df['age10'] = (ages // 10) * 10\n",
    "    \n",
    "    # Sexe\n",
    "    df['sexe'] = np.random.choice(['M', 'F'], n, p=[0.48, 0.52])\n",
    "    \n",
    "    # Dates\n",
    "    dates_entree = pd.date_range('2023-01-01', periods=n, freq='H')\n",
    "    durees = np.random.exponential(5, n).astype(int) + 1\n",
    "    dates_sortie = dates_entree + pd.to_timedelta(durees, unit='D')\n",
    "    \n",
    "    df['entree_date_ymd'] = dates_entree.strftime('%Y-%m-%d')\n",
    "    df['entree_date_ym'] = dates_entree.strftime('%Y-%m')\n",
    "    df['entree_date_y'] = dates_entree.strftime('%Y')\n",
    "    \n",
    "    df['sortie_date_ymd'] = dates_sortie.strftime('%Y-%m-%d')\n",
    "    df['sortie_date_ym'] = dates_sortie.strftime('%Y-%m')\n",
    "    df['sortie_date_y'] = dates_sortie.strftime('%Y')\n",
    "    \n",
    "    # Modes d'entrée/sortie\n",
    "    df['entree_mode'] = np.random.choice(['Urgence', 'Programmé', 'Transfert'], n, p=[0.6, 0.3, 0.1])\n",
    "    df['sortie_mode'] = np.random.choice(['Domicile', 'Transfert', 'Décès'], n, p=[0.85, 0.1, 0.05])\n",
    "    \n",
    "    # Spécialité\n",
    "    specialites = ['Cardio', 'Pneumo', 'Gastro', 'Neuro', 'Trauma', 'Onco', 'Psych']\n",
    "    df['specialite'] = np.random.choice(specialites, n)\n",
    "    \n",
    "    # Chirurgie\n",
    "    df['chirurgie'] = np.random.choice([0, 1], n, p=[0.7, 0.3])\n",
    "    \n",
    "    # Pathologies\n",
    "    df['diabete'] = np.random.choice([0, 1], n, p=[0.85, 0.15])\n",
    "    df['insuffisance_renale'] = np.random.choice([0, 1], n, p=[0.9, 0.1])\n",
    "    df['demence'] = np.random.choice([0, 1], n, p=[0.95, 0.05])\n",
    "    \n",
    "    # Diagnostics simulés (CIM10)\n",
    "    codes_cim10 = ['I10', 'E11', 'J44', 'C50', 'F32', 'N18', 'I25']\n",
    "    df['liste_diag'] = df.apply(lambda x: ', '.join(np.random.choice(codes_cim10, \n",
    "                                                                     np.random.randint(1, 4))), axis=1)\n",
    "    \n",
    "    # Actes simulés (CCAM)\n",
    "    codes_ccam = ['AAFA', 'BBFB', 'CCFC', 'DDFD', 'EEFE']\n",
    "    df['liste_acte'] = df.apply(lambda x: ', '.join(np.random.choice(codes_ccam, \n",
    "                                                                     np.random.randint(0, 3))) \n",
    "                                if x['chirurgie'] == 1 else '', axis=1)\n",
    "    \n",
    "    # Réordonner les colonnes (aléatoirement sauf pour les variables imbriquées)\n",
    "    colonnes = list(df.columns)\n",
    "    colonnes.remove('id_sejour')\n",
    "    colonnes.remove('age')\n",
    "    colonnes.remove('age5')\n",
    "    colonnes.remove('age10')\n",
    "    \n",
    "    np.random.shuffle(colonnes)\n",
    "    \n",
    "    # Remettre les variables d'âge dans l'ordre\n",
    "    colonnes = ['age10', 'age5', 'age'] + colonnes\n",
    "    \n",
    "    df = df[['id_sejour'] + colonnes]\n",
    "    \n",
    "    return df"
   ]
  },
